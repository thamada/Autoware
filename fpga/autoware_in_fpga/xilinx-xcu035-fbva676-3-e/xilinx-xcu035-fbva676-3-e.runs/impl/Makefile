# top.vdi
# top.tcl

all:
	vivado -log top.vdi -applog -m64 -product Vivado -messageDb vivado.pb -mode batch -source top.tcl -notrace


clean:
	rm -rf *.pb *.rpt *.jou *.rpx *.dcp *.vdi .*.rst .Xil


